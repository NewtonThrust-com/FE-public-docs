    "/docs/sign": {
      "post": {
        "tags": [
          "documents"
        ],
        "summary": "Sign Document",
        "description": "Sign a document with OTP verification",
        "operationId": "signDocument",
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "otp",
                  "refNT",
                  "password",
                  "documentId"
                ],
                "properties": {
                  "otp": {
                    "type": "string",
                    "description": "One-time password for document signing",
                    "example": "123456"
                  },
                  "refNT": {
                    "type": "string",
                    "description": "NewtonThrust reference code",
                    "example": "NT123456"
                  },
                  "password": {
                    "type": "string",
                    "description": "User password",
                    "format": "password"
                  },
                  "documentId": {
                    "type": "string",
                    "description": "The document ID to sign",
                    "example": "123"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Document signed successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Document signed successfully"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid OTP"
          },
          "404": {
            "description": "Document not found"
          }
        }
      }
    },
